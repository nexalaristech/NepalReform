{
  "name": "nepal-reforms-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build --webpack",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build --webpack",
    "analyze:server": "BUNDLE_ANALYZE=server next build --webpack",
    "build": "next build --webpack",
    "build:turbo": "next build",
    "dev": "next dev --turbopack",
    "dev:webpack": "next dev",
    "env:pull": "npx vercel env pull .env.local --yes",
    "lint": "next lint",
    "seo:check": "next build --webpack && next start",
    "sitemap:generate": "node scripts/generate-sitemap.js",
    "start": "next start",
    "docker:build": "docker build -t nepal-reforms:latest .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t nepal-reforms:dev .",
    "docker:build:lambda": "docker build -f Dockerfile.lambda -t nepal-reforms:lambda .",
    "docker:build:all": "./scripts/docker/build.sh all",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up web-dev",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up web",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker system prune -f",
    "docker:health": "curl -f http://localhost:3000/api/health || exit 1",
    "lambda:deploy": "./scripts/docker/deploy-lambda.sh",
    "lambda:test": "docker run -p 9000:8080 nepal-reforms:lambda"
  },
